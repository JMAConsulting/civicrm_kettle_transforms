  <step>
    <name>CiviCRM db - email</name>
    <type>TableInput</type>
    <description />
    <distribute>N</distribute>
    <custom_distribution />
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name />
    </partitioning>
    <connection>Target CiviCRM</connection>
    <sql>select contact_id, email AS contact_value, "Email" as contact_method, "" AS contact_type, clt.name AS contact_usage, is_primary FROM civicrm_email ce JOIN civicrm_contact cc ON ce.contact_id = cc.id 
LEFT JOIN civicrm_location_type clt ON ce.location_type_id = clt.id
WHERE is_deleted = 0
ORDER BY contact_id;</sql>
    <limit>0</limit>
    <lookup />
