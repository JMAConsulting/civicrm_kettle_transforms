    </partitioning>
    <connection>Target CiviCRM</connection>
    <sql>select contact_id, phone as contact_value, "Phone" as contact_method, cov.label AS contact_type, clt.name AS contact_usage, is_primary FROM civicrm_phone cp JOIN civicrm_contact cc ON cp.contact_id = cc.id 
LEFT JOIN civicrm_location_type clt ON cp.location_type_id = clt.id
LEFT JOIN civicrm_option_value cov ON cp.phone_type_id = cov.value
JOIN civicrm_option_group cog ON cov.option_group_id = cog.id AND cog.name = 'phone_type'
WHERE is_deleted = 0
ORDER BY contact_id;</sql>
