        <jsScript_script>//Subtract a year, unless it's a two-year or four-year membership
var join_date;
var end_date;
if (end_date &amp;&amp; !join_date) {
  end_year = end_date.slice(-4);
  if (MCAT_DESCR == 'Four Year Membersship') {
    join_year = end_year - 4;
  } else if (MCAT_DESCR == 'Two Year Membership') {
    join_year = end_year - 2;
  } else {
    join_year = end_year - 1;
  }
    
  join_date = end_date.slice(0, end_date.length-4) + join_year;
}

if (!join_date &amp;&amp; !end_date) {
  join_date = '01/01/2001';
  end_date = '01/01/2001';
}</jsScript_script>
